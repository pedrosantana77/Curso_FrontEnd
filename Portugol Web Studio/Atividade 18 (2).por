programa {
  funcao inicio() {
  inteiro numero1

  escreva("Informe um valor entre 100 e 200: ")
  leia(numero1)

  se ( numero1 >= 100 e numero1 <= 200 ) {
  escreva("O numero informado está no intervalo entre 100 e 200 ")
  }
  senao {
  escreva("O numero informado NÃO está no intervalo entre 100 e 200") 
  }
  }
}
